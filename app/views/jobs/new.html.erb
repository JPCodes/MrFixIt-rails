<h1>Post your Job</h1>
<%= render "layouts/errors", :object => @job %>

<%= form_for @job do |f| %>
  <%= f.label :title %>
<%= f.text_field :title, class: 'form-control' %>
  <%= f.label :description %>
  <%= f.text_field :description, class: 'form-control' %><br>
  <%= f.submit class: 'btn btn-primary'  %>
<% end %>
